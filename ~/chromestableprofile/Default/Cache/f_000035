    "// Generated by CoffeeScript 1.7.1\ndefine('modules/clean/history',['external/modernizr', 'modules/core/browser', 'modules/core/exception', 'modules/core/uri'], function(modernizr, Browser, _arg, URI) {\n  var DBHistory, assert;\n  assert = _arg.assert;\n  DBHistory = (function() {\n    var URL_ESCAPE_REGEX, _callbacks, _check_prefix_change, _check_url, _construct_url, _deconstruct_url, _exit_callbacks, _fire_callbacks, _fire_exit_callbacks, _get_prefix, _get_url, _internal_url, _urlwatcher;\n    URL_ESCAPE_REGEX = new RegExp(\"#|;|\\\\?|:|@|&|=|\\\\+|\\\\$\");\n    _get_url = function() {\n      var path, qargs, url;\n      url = void 0;\n      if (Modernizr.history || Browser.get_href().indexOf(\"#!\") === -1) {\n        path = window.location.pathname;\n        qargs = window.location.search;\n        if (path.search(URL_ESCAPE_REGEX) !== -1) {\n          path = String(URI({\n            path: path\n          }));\n        }\n        url = path + qargs;\n      } else {\n        url = Browser.get_href().split(\"#!\").pop();\n      }\n      return url;\n    };\n    _construct_url = function(path, qargs) {\n      return String(URI.parse(path).setQuery(qargs));\n    };\n    _deconstruct_url = function(url) {\n      var url_parts;\n      if (url == null) {\n        url = _get_url();\n      }\n      url_parts = url.split(\"?\");\n      return {\n        url: url,\n        path: url_parts[0],\n        qargs: URI.parse(url).getQuery()\n      };\n    };\n    _callbacks = {};\n    _exit_callbacks = {};\n    _internal_url = _get_url();\n    _urlwatcher = null;\n    _get_prefix = function(path) {\n      return \"/\" + path.split(\"/\")[1];\n    };\n    _fire_callbacks = function() {\n      var prefix, state, url;\n      url = _internal_url;\n      state = _deconstruct_url(url);\n      prefix = _get_prefix(state.path);\n      if (prefix in _callbacks) {\n        _callbacks[prefix](state.path.substr(prefix.length + 1), state.qargs);\n        return true;\n      }\n      r