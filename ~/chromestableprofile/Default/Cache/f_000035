() {\n            return clearInterval(_prefetch_interval_id);\n          });\n          player.on(\"ended\", function() {\n            return player.src({\n              type: options.type,\n              src: options.src\n            });\n          });\n        }\n        if (options.type === \"application/vnd.apple.mpegurl\" && Constants.new_web_player) {\n          _log_video_experience(player);\n        }\n        display_message = function() {\n          if (Constants.new_web_player) {\n            return player.addClass(\"show-truncated-bar\");\n          } else {\n            return $j(\"#truncated-video-message-bar\").text(truncated_msg).css('display', 'inherit');\n          }\n        };\n        if (!options.metadata && options.metadata_link) {\n          return ajax.AuthenticatedRequest({\n            url: options.metadata_link,\n            type: \"GET\",\n            data: {},\n            xhrFields: {\n              withCredentials: true\n            },\n            success: function(response) {\n              if (typeof options.metadata_cb === \"function\") {\n                options.metadata_cb(response);\n              }\n              if (response.is_truncated) {\n                return display_message.defer();\n              }\n            }\n          });\n        } else if (options.metadata && options.metadata.is_truncated) {\n          return display_message.defer();\n        }\n      };\n      truncated_msg = _(\"The clip displayed here is a preview. \" + \"To view the entire video, download or add it to your Dropbox.\");\n      if (Constants.new_web_player) {\n        if (Constants.transcoder_hls4web) {\n          videojs.Hls.GOAL_BUFFER_LENGTH = 120;\n        }\n        videojs.TruncatedBar = videojs.Component.extend();\n        videojs.TruncatedBar.prototype.options_ = {\n          loadEvent: \"play\",\n          components: {}\n        };\n        videojs.TruncatedBar.prototype.createEl = function() {\n          var el;\n          el = vi