timing_segment_began = null;\n      xhr_errors = [];\n      return percentage_watched = 100;\n    };\n    _onEnded = function() {\n      _pauseTiming();\n      return report();\n    };\n    _onShutdown = function() {\n      _pauseTiming();\n      percentage_watched = Math.round(player.currentTime() / player.duration() * 100);\n      if (!isFinite(percentage_watched)) {\n        percentage_watched = 0;\n      }\n      return report();\n    };\n    _resumeTiming = function() {\n      if (!timing_paused) {\n        return;\n      }\n      timing_segment_began = Date.now();\n      return timing_paused = false;\n    };\n    _pauseTiming = function() {\n      var delta;\n      if (timing_paused) {\n        return;\n      }\n      delta = Date.now() - timing_segment_began;\n      total_played_time += delta;\n      bitrate_times[level2bitrate[current_level].toString()] += delta;\n      return timing_paused = true;\n    };\n    _onPlay = function() {\n      if (first_play_time === null) {\n        last_quality_event = first_play_time = Date.now();\n      }\n      return last_user_interference = Date.now();\n    };\n    _onCanPlay = function() {\n      var current_bw_selected;\n      if (Constants.new_web_player) {\n        current_bw_selected = player.hls.selectPlaylist().attributes.BANDWIDTH / 1000;\n        current_level = level2bitrate.indexOf(current_bw_selected);\n        initial_bitrate = current_level;\n      }\n      _resumeTiming();\n      if (first_play) {\n        return;\n      }\n      initial_play_wait = Date.now() - first_play_time;\n      return first_play = true;\n    };\n    _onPause = function() {\n      return _pauseTiming();\n    };\n    _onSeeking = function() {\n      return last_user_interference = Date.now();\n    };\n    _onWaiting = function() {\n      _pauseTiming();\n      if (Date.now() - last_user_interference > 5000) {\n        return stalls += 1;\n      }\n    };\n    _onQualitySwitch = function(e) {\n      var old_level;\n      if (player.paused()) {\n        return;\n      }\n      _pauseTiming();\n      old_level = current_level;\n      current_level = e.data.level;\n      if (Date.now() - first_play_time < 1000) {\n        initial_bitrate = current_level;\n      } else {\n        quality_events.push([level2bitrate[old_level], level2bitrate[current_level], Date.now() - last_quality_event]);\n      }\n      last_quality_event = Date.now();\n      return _resumeTiming();\n    };\n    _onQualityChange = function() {\n      var current_bw_selected, old_level;\n      if (player.ended() || player.error() || player.paused()) {\n        return;\n      }\n      _pauseTiming();\n      current_bw_selected = player.hls.selectPlaylist().attributes.BANDWIDTH / 1000;\n      current_level = level2bitrate.indexOf(current_bw_selected);\n      old_level = current_level;\n      if (Date.now() - first_play_time < 1000) {\n        initial_bitrate = current_level;\n      } else {\n        quality_events.push([level2bitrate[old_level], level2bitrate[current_level], Date.now() - last_quality_event]);\n      }\n      last_quality_event = Date.now();\n      return _resumeTiming();\n    };\n    if (Constants.new_web_player) {\n      player.on(\"play\", _onPlay);\n      player.on(\"pause\", _onPause);\n      player.on(\"canplay\", _onCanPlay);\n      player.on(\"waiting\", _onWaiting);\n      player.on(\"seeking\", _onSeeking);\n      player.on(\"ended\", _onEnded);\n      player.on(\"shutdown\", _onShutdown);\n      player.on(\"mediachange\", _onQualityChange);\n      if (player.techName === \"Hls\") {\n        oldxhr = videojs.Hls.xhr;\n        return videojs.Hls.xhr = function(url, old_callback) {\n          var callback;\n          callback = function(error, url) {\n            var text;\n            if (error) {\n              text = \"\";\n              if (this.responseType === \"\" || this.responseType === \"text\") {\n                text = this.responseText;\n              }\n              xhr_errors.push([this.status.toString(), text, url]);\n            }\n            return old_callback.call(this, error, url);\n          };\n          return oldxhr.call(this, url, callback);\n        };\n      }\n    } else {\n      player.addEvent(\"play\", _onPlay);\n      player.addEvent(\"pause\", _onPause);\n      player.addEvent(\"canplay\", _onCanPlay);\n      player.addEvent(\"waiting\", _onWaiting);\n      player.addEvent(\"seeking\", _onSeeking);\n      player.addEvent(\"qualityswitch\", _onQualitySwitch);\n      return player.addEvent(\"ended\", _onEnded);\n    }\n  };\n  window.VideoUtil = VideoUtil;\n  return VideoUtil;\n});\n\n//# sourceMappingURL=video_util.map\n;\n",
